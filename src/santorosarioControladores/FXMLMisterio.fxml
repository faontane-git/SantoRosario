<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="santorosarioControladores.FXMLMisterioController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="700.0">
         <children>
            <Label fx:id="txtTitulo" alignment="CENTER" prefHeight="30.0" prefWidth="481.0" text="Hoy toca Rezar los Misterios XXXXXX">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </children></HBox>
      <VBox layoutX="272.0" layoutY="154.0" prefHeight="320.0" prefWidth="181.0">
         <children>
            <Button fx:id="bntMisterioHoy" mnemonicParsing="false" onAction="#clickMisterioHoy" prefHeight="85.0" prefWidth="180.0" text="Rezar Misterio de Hoy">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="btnOtroMisterio" mnemonicParsing="false" onAction="#clickOtroMisterio" prefHeight="85.0" prefWidth="180.0" text="Otro Misterio">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
