<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="santorosarioControladores.FXMLDesicionController">
   <children>
      <HBox alignment="CENTER" layoutY="-1.0" prefHeight="102.0" prefWidth="700.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="30.0" prefWidth="246.0" text="¿Qué Desea Rezar hoy?">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <VBox alignment="CENTER" layoutX="235.0" layoutY="104.0" prefHeight="393.0" prefWidth="216.0">
         <children>
            <Button fx:id="btnSantoRosario" mnemonicParsing="false" onAction="#ClickSantoRosario" prefHeight="85.0" prefWidth="190.0" text="Santo Rosario" />
            <Button fx:id="btnDivinaMisericordia" mnemonicParsing="false" onAction="#clickDivinaMisericordia" prefHeight="85.0" prefWidth="190.0" text="Coronilla a la Divina Misericordia">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <Button fx:id="btnMenu" layoutX="617.0" layoutY="14.0" mnemonicParsing="false" onAction="#clickMenu" prefHeight="37.0" prefWidth="69.0" text="Menú" />
   </children>
</AnchorPane>
